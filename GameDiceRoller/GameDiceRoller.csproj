<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>GameDiceRoller</PackageId>
        <PackageVersion>1.0.0</PackageVersion>
        <Authors>Fulmetal</Authors>
        <Product>GameDiceRoller</Product>
        <Description>A library for simulating dice rolls in games.</Description>
        <Copyright>Copyright (c) Your Name</Copyright>
        <PackageProjectUrl>https://github.com/Fulmetal/GameDiceRoller</PackageProjectUrl>
        <PackageLicense>https://github.com/Fulmetal/GameDiceRoller/blob/main/LICENSE</PackageLicense>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <RepositoryUrl>https://github.com/Fulmetal/GameDiceRoller</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>dice game roll RPG</PackageTags>
        <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    </PropertyGroup>

</Project>
